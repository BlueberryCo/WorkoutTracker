<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blueberryco.workouttracker" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="blueberryco.workouttracker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="blueberryco.workouttracker.ClientProfileActivity" >
        </activity>
        <activity android:name="blueberryco.workouttracker.ClientStatsActivity" >
        </activity>
        <activity android:name="blueberryco.workouttracker.TrainerActivity" >
        </activity>
        <activity android:name="blueberryco.workouttracker.WorkoutDetailsActivity" >
        </activity>
    </application>

</manifest>
