<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blueberryco.workouttracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClientScheduleActivity"></activity>
        <activity android:name=".ClientStatsActivity"></activity>
        <activity android:name=".TrainerActivity"></activity>
        <activity android:name=".WorkoutDetailsActivity"></activity>
        <activity android:name=".ExpandableListMain">
        </activity>
        <activity
            android:name=".CreateOwnerActivity"
            android:label="@string/title_activity_create_owner"></activity>
        <activity android:name=".ClientsActivity"></activity>
        <activity android:name=".BasicExercisesAcivity"></activity>
        <activity android:name=".InfoActivity"></activity>
        <activity android:name=".AddWorkout"></activity>
        <activity android:name=".ClientProfile"></activity>
    </application>

</manifest>
